    "special": [
        {
            "name": "Bravery",
            "description": "+1 to Will saves vs fear",
            "notes": ""
        },
        {
            "name": "Close Control",
            "description": "At 2nd level, a brawler becomes skilled at forcefully moving his opponent around the battlefield. The brawler gains a +1 bonus on bull rush, drag, and reposition combat maneuver checks. The brawler also gains a +1 bonus to CMD when attacked with the bull rush, drag, and reposition maneuvers. These bonuses increase by +1 for every four levels after 2nd (to a maximum of +5 at 18th level). ",
            "notes": ""
        },
        {
            "name": "Close Combatant",
            "description": "At 3rd level, a brawler gains a +1 bonus on attack rolls and a +3 bonus on damage rolls with weapons in the close weapon group. Both of these bonuses increase by +1 for every four levels beyond 3rd (to a maximum of +5 on attack rolls and +7 on damage rolls at 19th level).",
            "notes": "(bonuses applied to current attacks)"
        }
    ],
    "traits": [
        {
            "name": "No Escape",
            "description": "+1 to CMB checks to restrain witnessed lawbreakers",
            "notes": ""
        },
        {
            "name": "Honeyed Tongue",
            "description": "+1 to diplomacy checks (applied); diplomacy becomes a class skill",
            "notes": ""
        }
    ],
    "feats": [
        {
            "name": "Fast Learner",
            "description": "Get both +1 HP and +1 skill point on leveling",
            "notes": ""
        },
        {
            "name": "Cunning",
            "description": "+1 skill point per level",
            "notes": ""
        },
        {
            "name": "Combat Reflexes",
            "description": "You may make a number of additional attacks of opportunity per round equal to your Dexterity bonus. With this feat, you may also make attacks of opportunity while flat-footed.",
            "notes": ""
        },
        {
            "name": "Enforcer",
            "description": "Whenever you deal nonlethal damage with a melee weapon, you can make an Intimidate check to demoralize your target as a free action. If you are successful, the target is shaken for a number of rounds equal to the damage dealt. If your attack was a critical hit, your target is frightened for 1 round with a successful Intimidate check, as well as being shaken for a number of rounds equal to the damage dealt.",
            "notes": ""
        },
        {
            "name": "Alertness",
            "description": "+2 to Perception and Sense Motive checks (applied)",
            "notes": "If you have 10 or more ranks in one of these skills, the bonus increases to +4 for that skill."
        },
        {
            "name": "Uncanny Alertness",
            "description": "+1 to Perception and Sense Motive checks (applied), and +2 to saving throws against sleep effects",
            "notes": ""
        },
        {
            "name": "Power Attack",
            "description": "You can choose to take a \u20131 penalty on all melee attack rolls and combat maneuver checks to gain a +2 bonus on all melee damage rolls. This bonus to damage is increased by half (+50%) if you are making an attack with a two-handed weapon, a one handed weapon using two hands, or a primary natural weapon that adds 1-1/2 times your Strength modifier on damage rolls. This bonus to damage is halved (\u201350%) if you are making an attack with an off-hand weapon or secondary natural weapon.",
            "notes": "When your base attack bonus reaches +4, and every 4 points thereafter, the penalty increases by \u20131 and the bonus to damage increases by +2."
        }
    ],
